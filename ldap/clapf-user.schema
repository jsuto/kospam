# Attribute Type Definitions


attributetype ( 1.3.6.1.4.1.7914.1.2008.1.1 NAME 'policyGroupId'
        DESC 'The maximum size of a single messages the user accepts.'
        EQUALITY integerMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE )

attributetype ( 1.3.6.1.4.1.7914.1.2008.1.2 NAME 'isAdmin'
        DESC 'Is this an administrative user or not.'
        EQUALITY integerMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE )

attributetype ( 1.3.6.1.4.1.7914.1.2008.1.3 NAME 'blackList'
        DESC 'blacklisted email address patterns.'
        EQUALITY caseIgnoreIA5Match
        SUBSTR caseIgnoreIA5SubstringsMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{512} )

attributetype ( 1.3.6.1.4.1.7914.1.2008.1.4 NAME 'proxydn'
        DESC 'dn on the remote AD/LDAP host.'
        EQUALITY caseIgnoreIA5Match
        SUBSTR caseIgnoreIA5SubstringsMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{512} )

attributetype ( 1.3.6.1.4.1.7914.1.2008.1.5 NAME 'domain'
        DESC 'domain part from the user@domain address.'
        EQUALITY caseIgnoreIA5Match
        SUBSTR caseIgnoreIA5SubstringsMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{512} )

attributetype ( 1.3.6.1.4.1.7914.1.2008.1.6 NAME 'clapfgid'
        DESC 'clapf group id.'
        EQUALITY integerMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE )


objectclass ( 1.3.6.1.1.123456.6.0 NAME 'clapfUser'
        DESC 'clapf User'
        SUP top
        AUXILIARY
	MAY ( policygroupid $ isAdmin $ blackList $ proxydn $ domain $ clapfgid )
        )

